{
  "name": "nodejs-interview",
  "version": "1.0.0",
  "description": "auth system ",
  "main": "src/main.ts",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --exec 'ts-node -r tsconfig-paths/register' src/main.ts",
    "build": "npx tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "npx ts-node src/main.ts",
    "setup": "npm i && npm run dev",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:generate": "typeorm migration:generate -d ormconfig.ts src/migrations/",
    "migration:run": "typeorm migration:run -d ormconfig.ts",
    "migration:revert": "npm run typeorm --migration:revert -d ./src/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "seed": "ts-node-dev src/seeders/0001-SeedAdmin.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "authentication"
  ],
  "author": "Davz",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.18",
    "@types/socket.io": "^3.0.2",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cli-color": "^2.0.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.23",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.6",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
